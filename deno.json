{
  "tasks": {
    "test": "deno test --allow-all --doc --parallel --coverage --clean",
    "ok": "deno lint && deno fmt --check && deno task test",
    "bot": "deno task --cwd bot start",
    "www:dev": "deno task --cwd www serve",
    "www:build": "deno task --cwd www build",
    "www:prod": "deno task --cwd www prod"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.7",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@std/fs": "jsr:@std/fs@^1.0.5",
    "@std/http": "jsr:@std/http@^1.0.9",
    "@std/log": "jsr:@std/log@^0.224.9",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "discord.js": "npm:discord.js@^14.16.3",
    "tailwindcss": "npm:tailwindcss@^3.4.14",
    "lume/": "https://deno.land/x/lume@v2.4.2/"
  },
  "exclude": [
    "coverage",
    "_site"
  ],
  "workspace": [
    "./www",
    "./bot"
  ],
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "precompile",
    "jsxImportSource": "npm:preact"
  }
}
